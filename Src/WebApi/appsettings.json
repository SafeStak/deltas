{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "KeyVaultConfiguration": {
    "Url": "https://pf-live-eun-pf-api-kv.vault.azure.net/",
    "ClientId": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
    "SecretName": "poolfolio-api"
  },
  "PoolApiSettings": {
    "BaseUrl": "https://js.adapools.org/"
  },
  "CacheConfiguration": {
    "RedisCacheConnectionKey": "RedisConnectionString",
    "PoolListTtl": "00:03",
    "PoolGetTtl": "00:01",
    "PoolGetTtlMaxJitter": "00:01",
    "RetryPolicy": {
      "MaxRetryAttempts": 5,
      "BackOffBaseMilliseconds": 50
    },
    "CircuitBreakerPolicy": {
      "FailureThreshold": 0.5,
      "SamplingDuration": 10,
      "MinimumThroughput": 8,
      "BreakDuration": 30
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
    "DisableTelemetry": "true"
  },
  "AllowedHosts": "*"
}
